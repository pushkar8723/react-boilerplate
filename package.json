{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplace to easily bootstrap a project.",
  "main": "src/index.html",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode development --inline --open --hot",
    "start-prod": "NODE_ENV=production webpack-dev-server --mode production",
    "build": "NODE_ENV=production webpack --mode production",
    "lint": "tslint -p tsconfig.json",
    "lint-staged": "lint-staged",
    "test": "jest --coverage",
    "check-all": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushkar8723/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Pushkar Anand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pushkar8723/react-boilerplate/issues"
  },
  "homepage": "https://github.com/pushkar8723/react-boilerplate#readme",
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-google-login": "^5.0.4",
    "sleek-ui": "^3.0.110",
    "sparkx": "^0.0.13",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/styled-components": "^4.1.13",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@babel/types": "^7.0.0-beta.36",
    "@types/jest": "^24.0.12",
    "and": "0.0.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^24.8.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14"
  },
  "lint-staged": {
    "*.{ts,tsx}": "tslint -p tsconfig.json"
  },
  "pre-commit": "check-all",
  "jest": {
    "moduleNameMapper": {
      "^core(.*)$": "<rootDir>/src/core$1",
      "^services(.*)$": "<rootDir>/src/services$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^views(.*)$": "<rootDir>/src/views$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^model(.*)$": "<rootDir>/src/model$1",
      "^components(.*)$": "<rootDir>/src/components$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ]
  }
}
