{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-airbnb"
    ],
    "rules": {
        "ter-indent": [true, 4],
        "completed-docs": true,
        "jsdoc-format": true,
        "no-submodule-imports": false,
        "no-implicit-dependencies": false
    }
}